{
      "level": 1,
      "name": "SayaManager",
      "display_name": "插件管理",
      "version": "0.1",
      "authors": [
            "十三"
      ],
      "description": "用于管理插件",
      "usage": [
            "发送 '开启/关闭插件 {编号}' 来修改已加载插件的可用状态",
            "发送 '卸载/重载插件 {编号}' 来卸载/重载已加载插件",
            "发送 '加载插件 {编号}' 来加载未加载插件"
      ],
      "example": [
            "加载插件 1",
            "重载插件 2",
            "卸载插件 3",
            "开启插件 4",
            "关闭插件 5"
      ],
      "default_switch": true,
      "default_notice": false
}