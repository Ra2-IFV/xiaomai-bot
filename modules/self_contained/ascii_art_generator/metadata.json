{
  "level": 1,
  "name": "ASCIIArtGenerator",
  "version": "0.1",
  "display_name": "字符画生成器",
  "authors": [
    "13"
  ],
  "description": "生成字符画",
  "usage": [
    "指令:",
    "-ascii / 字符画, 在同一条讯息中附上图片",
    "参数:",
    "-density, -d: 设定字符的密度, [low mid high]",
    "-brightness, -b: 设定亮度, float",
    "-contrast, -c: 设定对比度增强, float",
    "-invert, -i: 设定是否反转字符集, boolean",
    "-width, -w: 设定生成图像的宽度 (字符数), int"
  ],
  "example": [
    "-ascii + 图片",
    "-ascii -d low -b 0.9",
    "字符画 -contrast 1.14 -width 514 -invert"
  ],
  "default_switch": true,
  "default_notice": false
}
